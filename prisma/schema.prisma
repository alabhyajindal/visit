generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Visit {
  id    String    @id @default(cuid())
  image String?
  title String
  description String
  location String
  maxGuests Int
  duration Int
  price   Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}